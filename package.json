    "express-validator": "^6.5.0",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.9.19"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4"
  }
}
